omponents/main-view/main-view.jsx","original":{"line":71,"column":3}},{"name":"React","generated":{"line":119,"column":2},"source":"components/main-view/main-view.jsx","original":{"line":6,"column":30}},{"name":"Component","generated":{"line":119,"column":17},"source":"components/main-view/main-view.jsx","original":{"line":6,"column":36}}],"sources":{"components/main-view/main-view.jsx":"import React from 'react';\r\nimport axios from 'axios';\r\nimport { MovieCard } from '../movie-card/movie-card';\r\nimport { MovieView } from '../movie-view/movie-view';\r\n\r\nexport class MainView extends React.Component {\r\n  constructor() {\r\n    //Call the superclass constructor\r\n    // so React can initialize it\r\n    super();\r\n\r\n    // Initialize the state to an empty object so we can destructure it later\r\n    this.state = {\r\n      movie: null,\r\n      selectedMovie: null\r\n    };\r\n  }\r\n\r\n  //One fot the \"hooks\" available in a react component\r\n  componentDidMount() {\r\n    axios\r\n      .get('https://myflixdbjcg.herokuapp.com/movies')\r\n      .then(response => {\r\n        this.setState({\r\n          movies: response.data\r\n        });\r\n      })\r\n      .catch(function (error) {\r\n        console.log(error);\r\n      });\r\n  }\r\n\r\n  onMovieClick(movie) {\r\n    this.setState({\r\n      selectedMovie: movie\r\n    });\r\n  }\r\n\r\n  onBackClick(movie) {\r\n    this.setState({\r\n      selectedMovie: null\r\n    });\r\n  }\r\n\r\n  render() {\r\n    // If the state isn't initialized, this will throw on runtime\r\n    // before the data is initially loaded\r\n    const { movies, selectedMovie } = this.state;\r\n\r\n    //Before the movies have been loaded\r\n    if (!movies) return <div className='main-view' />;\r\n\r\n    return (\r\n      <div className='main-view'>\r\n        {selectedMovie ? (\r\n          <MovieView\r\n            movie={selectedMovie}\r\n            onClick={button => this.onBackClick()}\r\n          />\r\n        ) : (\r\n            movies.map(movie => (\r\n              <MovieCard\r\n                key={movie._id}\r\n                movie={movie}\r\n                onClick={movie => this.onMovieClick(movie)}\r\n              />\r\n            ))\r\n          )}\r\n      </div>\r\n    );\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"74f5efb539af66ca2d84ddb564a8f9a0","cacheData":{"env":{}}}